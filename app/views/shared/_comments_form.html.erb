<div class="row">
  <%= form_for @comment do |f| %>
    <div class="form-group text-center">
      <%= f.label :feedback, "Leave a Feedback" %><br>
      <%= f.text_area :text, rows: 10, class: "form-control" %><br>

      <%= f.submit("Submit", class: "btn btn-primary btn-center") %>
    </div>
  <% end %>
</div>
