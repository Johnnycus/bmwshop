<% title "Profile" %><br><br><br><br><br><br><br><br><br><br>
<div class="container text-center">
  <div class="row">
    <div class="col-md-4 col-md-offset-4 well">
      <h1>Profile</h1>

      <h3>Email: <strong><%= @user.email %></strong></h3>
      <h3>Date of sign up: <strong><%= @user.created_at.strftime('%b %d, %Y') %></strong></h3>

      <%= link_to "Edit", edit_user_registration_path, class: "btn btn-lg btn-default" %>
      <%= link_to "Back", :back, class: "btn btn-lg btn-default" %>
    </div>
  </div>
</div>
